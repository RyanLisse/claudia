# AI Agent System Environment Configuration

# Inngest Configuration
INNGEST_EVENT_KEY=your_inngest_event_key_here
INNGEST_SIGNING_KEY=your_inngest_signing_key_here
INNGEST_BASE_URL=https://api.inngest.com

# Agent System Configuration
AGENT_MAX_AGENTS=50
AGENT_TASK_QUEUE_SIZE=1000
AGENT_HEARTBEAT_INTERVAL_MS=30000
AGENT_TASK_TIMEOUT_MS=300000

# Database Configuration (for persistence)
DATABASE_URL=your_database_url_here

# Monitoring Configuration
ENABLE_MONITORING=true
MONITORING_ALERT_WEBHOOK=your_webhook_url_for_alerts
MONITORING_RETENTION_DAYS=30

# Performance Configuration
ENABLE_PERFORMANCE_METRICS=true
MAX_MEMORY_USAGE_MB=1024
MAX_CPU_USAGE_PERCENT=80

# Security Configuration
ENABLE_SECURITY_SCANNING=true
SECURITY_SCAN_TIMEOUT_MS=60000

# Development Configuration
NODE_ENV=development
LOG_LEVEL=info
DEBUG_AGENTS=false